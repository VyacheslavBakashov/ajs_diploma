{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "object-curly-newline": "off",
        "no-alert": 0,
        "no-plusplus": 0,
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": [
            "error",
            {"devDependencies": true}
        ],
        "no-param-reassign": ["error", { "props": false }]
    }
}
